{
  "testing.automaticallyOpenPeekView": "never",
  "editor.tabSize": 2,
  "files.eol": "\n",
  "files.trimTrailingWhitespace": true,
  "editor.rulers": [100],
  "search.exclude": {
    "**/coverage/": true,
    "**/dist/": true,
    "**/node_modules/": true
  },

  "typescript.suggest.completeFunctionCalls": true,
  "[typescript]": {
    "editor.codeActionsOnSave": ["source.fixAll.eslint"],
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true,
    "editor.tabSize": 2,
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true,
    "editor.tabSize": 2,
  },
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true,
    "editor.tabSize": 2,
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true,
    "editor.tabSize": 2,
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true,
    "editor.tabSize": 2,
},
"cSpell.words": [
  "archi",
  "betaprime",
  "Codeforces",
  "depcruise",
  "ERRCODE",
  "fillcolor",
  "fontawesome",
  "fortawesome",
  "fourist",
  "Geomean",
  "Geomeans",
  "gmtime",
  "gonal",
  "graphviz",
  "Idleforces",
  "infty",
  "IRL",
  "Iterableify",
  "jsons",
  "linebreak",
  "Logit",
  "maxx",
  "mktime",
  "mts",
  "neighbours",
  "NPC",
  "npcs",
  "npx",
  "Parens",
  "pcorr",
  "penwidth",
  "probs",
  "Pythonic",
  "reduxjs",
  "snapshotted",
  "STDEV",
  "Stdevs",
  "systests",
  "tsc",
  "UTF"
],
"cSpell.ignorePaths": [
    "package-lock.json",
    "node_modules",
    "vscode-extension",
    ".git/objects",
    ".vscode",
    ".vscode-insiders",
    "src/app/users/trimmedUsers.json"
],
"cSpell.minWordLength": 3,
}
